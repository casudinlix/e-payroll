select `db_hr_dev`.`tbl_employee`.`id` AS `id`,`db_hr_dev`.`tbl_employee`.`fid` AS `fid`,`db_hr_dev`.`tbl_users`.`username` AS `username`,`db_hr_dev`.`tbl_users`.`pass` AS `pass`,`db_hr_dev`.`tbl_users`.`group_id` AS `group_id`,`db_hr_dev`.`tbl_users`.`status` AS `status` ,tbl_employee.email from (`db_hr_dev`.`tbl_employee` join `db_hr_dev`.`tbl_users` on((`db_hr_dev`.`tbl_employee`.`id` = `db_hr_dev`.`tbl_users`.`id_emp`)))
